export PATH=$LOCAL_HOME/bin:$PATH

# brew
export PATH=/usr/local/bin:$PATH

# elasticsearch
export ELASTICROOT=/usr/local/Cellar/elasticsearch/6.6.0_1
export ELASTICPATH=/usr/local/var/elasticsearch/plugins/
export PATH=$ELASTICPATH:$PATH

# Node Path
export PATH=$PATH:${HOME}/.nodebrew/current/bin

# Go Path
if [ -x "`which go`" ]; then
    export GOROOT=`go env GOROOT`
    export GOPATH=$HOME/go
    export PATH=$PATH:$GOROOT/bin:$GOPATH/bin
fi

export GOSKELTON_DEST_DIR=$GOPATH/src/github.com/kimai07
export GOSKELTON_USER=kimai07
